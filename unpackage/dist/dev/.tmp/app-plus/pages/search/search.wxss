._a {
  box-sizing: border-box;
}
.page {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  /* background: #F7F7F7; */
  /* padding-bottom: 80px; */
}
.statusBar {

  display: block;
  position: fixed;
  height: var(--status-bar-height);
  width: 100%;
  top: 0;
  z-index: 1;
  background: #FFFFFF;

}
.header {

  margin-top: var(--status-bar-height);

  height: 170rpx;
  width: 100vw;
  background: #FFFFFF;
  z-index: 200;
  position: fixed;
  box-shadow: 0px 1px 5px 0px rgba(0, 0, 0, 0.3);
}
.header .title {
  font-weight: bold;
  position: fixed;
  /* height: 80rpx; */
  width: 100%;
  text-align: center;
  line-height: 80rpx;
}
/* 搜索栏 */
.search {
  height: 50rpx;
  width: 100vw;
}
.searchWrapper {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  margin: 0 20rpx 20rpx 20rpx;
  width: 95vw;
  top: 200;
  margin-top: 80rpx;
  margin-right: 20rpx;
  border-radius: 20px;
  overflow: hidden;
  border: 1px solid #eaeaea;
  /* background: #eaeaea82; */
}
.searchIconWrapper {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 10rpx 0;
}
.searchIconWrapper image {
  width: 40rpx;
  height: 40rpx;
  margin: 0rpx 10rpx;
}
.inputWrapper {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  /* background: #eaeaea82; */
  font-size: 14px;
  padding: 10rpx 0;
}
input {}
/* 搜索结果 */
.main {
  padding: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-top: 180rpx;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;

  margin-top: 120px;

}
.movie {
  margin-bottom: 40rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  background: #FFFFFF;
  padding: 20rpx;
  border-radius: 10rpx;
  box-shadow: 1px 1px 4px 1px rgba(0, 0, 0, 0.15);
}
.movieName {
  margin-top: 6rpx;
  font-size: 14px;
  font-weight: bold;
  margin-bottom: 20rpx;
  /*  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap; */
}
/* 视频信息 */
.detail {
  padding-bottom: 20rpx;
  border-bottom: 1px solid #EAEAEA;
}
.detail view {
  color: #848484;
  font-size: 14px;
}
.detail view text {
  color: black;
  margin-left: 16rpx;
}
.movieInfo {}
.toggleButton {
  display: -webkit-inline-box;
  display: -webkit-inline-flex;
  display: inline-flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.toggleShowInfo {
  color: #ffd655 !important;
}
.text {
  margin-right: 30rpx;
}
.toggleShowInfoIcon {
  width: 40rpx;
  height: 40rpx;
  margin-left: 6rpx;
}
.toggleShowInfoIcon.down {
  background: url(data:image/png;base64,) no-repeat 0 0;
  background-size: cover;
}
.toggleShowInfoIcon.up {
  background: url(data:image/png;base64,) no-repeat 0 0;
  background-size: cover;
}
/* 播放列表 */
.selectLink {
  margin: 10rpx 0 20rpx;
  font-size: 14px;
}
.playLink {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  position: relative;
  padding: auto;
  margin: 0 -12rpx;
}
.playLinkItem {
  /* flex: 1; */
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
          align-items: flex-start;
  text-align: center;
  margin: 12rpx;
  padding: 10rpx 20rpx;
  border-radius: 4px;
  border: 1px solid grey;
}
._a {
  color: black;
  text-decoration: none;
  font-size: 14px;
}
/* 未搜索时展示的数据 */
.defaultMovie .title{
  font-weight: bold;
  line-height: 1.8;
  font-size: 16px;
}
.defaultMovie .searchHistory{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  margin-bottom: 20rpx;
}
.defaultMovie .searchTerms{
  background: #f3f3f3;
  padding: 6rpx 12rpx;
  margin: 10rpx;
  border-radius: 4px;
  font-size: 14px;
  min-width: 60rpx;
  text-align: center;
}
.recommendMovie {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.recommendMovie .movie{
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  width: 33%;
  margin: 10rpx;
  padding: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient:vertical;
  -webkit-box-direction:normal;
  -webkit-flex-direction:column;
          flex-direction:column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center; 
  background: transparent;
  box-shadow: none;
}
.recommendMovie .movieCover{
  width: 200rpx;
  height: 270rpx;
}
.recommendMovie .movieName{
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  width: 200rpx;
}
